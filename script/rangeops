#!/usr/bin/perl
use strict;
use warnings;
use App::Rangeops;

App::Rangeops->run;

=head1 OVERVIEW

For help with App::Rangeops, run "rangeops help"

=cut

__END__

perl -I lib/ script/rangeops

perl -I lib/ script/rangeops sort t/I.links.tsv -o stdout
cat t/I.links.tsv \
    | perl -I lib/ script/rangeops sort stdin -o stdout

perl -I lib/ script/rangeops merge t/I.links.tsv -o stdout
cat t/I.links.tsv \
    | perl -I lib/ script/rangeops merge stdin -o stdout

perl -I lib/ script/rangeops clean t/I.sort.tsv -o stdout
perl -I lib/ script/rangeops clean t/I.sort.tsv -r t/I.merge.tsv -o stdout

perl -I lib/ script/rangeops merge t/I.lastz.tsv t/I.blast.tsv -o stdout

perl -I lib/ script/rangeops sort t/I.lastz.tsv t/I.blast.tsv -o stdout \
    | perl -I lib/ script/rangeops clean stdin -r t/I.merge.tsv -o stdout

